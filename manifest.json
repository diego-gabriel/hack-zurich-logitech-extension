{
  "name": "Wheel",
  "description": "Logitech challenge",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["activeTab", "declarativeContent", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [{
    "matches": ["https://*.google.com/search*"],
    "js": ["search.js"],
    "css": ["search.css"]
  }]
}